     "output_type": "display_data"
    }
   ],
   "source": [
    "it = 1500\n",
    "alpha = 0.01\n",
    "theta = np.zeros((n+1,1)) # theta initialised to zeroes\n",
    "\n",
    "theta = gDesc(X,theta,y,alpha,it)[0]\n",
    "cost = gDesc(X,theta,y,alpha,it)[1]\n",
    "\n",
    "print(theta)\n",
    "\n",
    "plt.plot(cost)\n",
    "plt.show()"
   ]
